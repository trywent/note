/*
 7862
*/
echo 1 > /proc/sys/kernel/printk

echo device > /sys/devices/platform/soc/soc:aon/5fff0000.usb/host_dev
echo host > /sys/devices/platform/soc/soc:aon/5fff0000.usb/host_dev
dd if=/sdcard/boot.img of=/dev/block/platform/soc/soc:ap-apb/71400000.sdio/by-name/boot
dd if=/sdcard/u-boot-sign.bin of=/dev/block/platform/soc/soc:ap-apb/71400000.sdio/by-name/uboot

dd if=/sdcard/dtbo.img of=/dev/block/platform/soc/soc:ap-apb/71400000.sdio/by-name/dtbo
dd if=/sdcard/recovery.img of=/dev/block/platform/soc/soc:ap-apb/71400000.sdio/by-name/recovery


lunch ums512_1h10_Natv-userdebug-native
m audio.primary.ums512
/data/vendor/local/media


setprop sys.fyt.bluetooth_type ""


//lesc
dd if=/dev/zero of=temp.img bs=1M count=32
mkfs.ext4 -F temp.img
//串口
mcu spi2/u2  pin脚A16 B16	apuart2
console 			apuart1
bt  u1  pin脚H5 J5		apuart0

/* 
 kernel android10的kernel编译从android结构中抽出 

*/

export BSP_KERNEL_DEFCONFIG="sprd_sharkl5Pro_defconfig"
@bsp/device/sharkl5Pro/androidq/ums512_1h10/ums512_1h10_base/common.cfg

export BSP_DTB="ums512-1h10"
export BSP_DTBO="ums512-1h10-overlay"
@@bsp/device/sharkl5Pro/androidq/ums512_1h10/ums512_1h10_Natv/common.cfg

//查找device,vendor目录下面的vendorsetup.sh
source_vendorsetup() {{
- for dir in device vendor product; do
	
}@build/envsetup.sh


//重写make命令
make{
- make $partition_type "$make_parameters"
- aosp_make $partition_type "$make_parameters"
}@vendorsetup.sh

aosp_make(){
- bash $ANDROID_BUILD_TOP/bsp/build/make_for_androidq.sh $@
}@vendorsetup.sh

bsp/build/make_for_androidq.sh
bsp/build/envsetup.sh
vendor/sprd/feature_configs/vendorsetup.sh


/*
 制作pac
*/
./vendor/sprd/release/IDH/Script/build_pac.sh –a ums512_1h10_Natv-userdebug-native –b BUILD –c ***
- 参数-a 是./vendor/sprd/release/IDH 目录下面 "sp*" 目录名称
- 参数-b BUILD|build:编译;PAC|pac:打 pac 包。
- 参数-c 是编译运营商工程,如果有多个用英文","隔开
