late_initcall(ts_init)-->ts_init{
  //获取dts信息
- ts_board_init(){
	- of_find_compatible_node(NULL, NULL, ATS_COMPATIBLE)	// "adaptive-touchscreen"
	- g_board = ts_parse_dt(pn)
	- ts_bus_init(pn->parent, board->controller == NULL)
	}@ats_loader.c
- platform_driver_register(&ats_driver)
}@ats_core.c

