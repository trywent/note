/************************
  wifi状态wifiStateMachine
*/

WifiStateMachine(mContext, mFrameworkFacade,wifiStateMachineLooper,this, mBackupManagerProxy, mCountryCode, mWifiNative)@WifiInjector.java
WifiStateMachine(Context context, FrameworkFacade facade, Looper looper, WifiInjector wifiInjector,
  BackupManagerProxy backupManagerProxy, WifiCountryCode countryCode, WifiNative wifiNative){
- mWifiInjector = wifiInjector
- mWifiNative = wifiNative
- mWifiStateTracker = wifiInjector.getWifiStateTracker()
- mWifiMonitor = mWifiInjector.getWifiMonitor()
- mWifiInfo = new WifiInfo()
  //state
- addState(mDefaultState) 
	addState(mInitialState, mDefaultState)
	addState(mSupplicantStartingState, mDefaultState)
- setInitialState(mInitialState)
- start()
}@WifiStateMachine.java



setSupplicantRunning-->sendMessage(CMD_START_SUPPLICANT/CMD_STOP_SUPPLICANT)

InitialState{//初始状态
- processMessage{
	- CMD_START_SUPPLICANT{ //打开wifi
		- mClientInterface = mWifiNative.setupForClientMode() //设置wifi模式为client
		- mWifiNative.enableSupplicant()
		- mWifiMonitor.startMonitoring
		}
	}
}


SoftApState{//AP热点状态
- enter{
	- IApInterface apInterface = mWifiNative.setupForSoftApMode()//设置热点
	- mSoftApManager = mWifiInjector.makeSoftApManager
	}
}


